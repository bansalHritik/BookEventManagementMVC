
@{
    ViewBag.Title = "All";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table table-striped table-bordered" id="events">
    <thead>
        <tr>
            <th>Event</th>
            <th>Event Type</th>
        </tr>
    </thead>
    <tbody></tbody>
</table>


@section scripts{

    <script>
        $('#events').DataTable({
            ajax: {
                url: '/api/events',
                dataSrc: '',
            },
            columns: [{
                data: "title",
            },
                {data:'type'
                }]
        })
    </script>
}